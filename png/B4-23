digraph finite_state_machine { rankdir=LR
	node [shape=doublecircle]
	0

	node [shape=circle]
	fake0 [style=invisible]
	fake0 -> 1

	1 -> 2 [label="a"] 
	2 -> 2 [label="a"] 
	2 -> 3 [label="b"] 
	3 -> 3 [label="b"] 
	3 -> 4 [label="c"] 
	4 -> 4 [label="c"] 
	4 -> 0 [label="d"] 
	0 -> 0 [label="d"] 
}
